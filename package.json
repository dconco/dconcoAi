	"author": "dconco",
	"dependencies": {
		"@google/genai": "^1.19.0",
		"dotenv": "^16.6.1",
		"express": "^4.21.2"
	},
	"devDependencies": {
		"@types/express": "^4.17.23",
		"@types/node": "^20.19.13",
		"ts-node-dev": "^2.0.0",
		"tsconfig-paths": "^4.2.0",
		"typescript": "^5.9.2"
	}
}
